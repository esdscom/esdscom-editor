@page "/Sections/SectionMenu"

@using Syncfusion.Blazor.Navigations
@using Syncfusion.Blazor.Buttons
@using Syncfusion.Blazor.Lists

<div id="sidebar-wrapper">
    <div>
        <SfToolbar CssClass="defaultToolbar">
            <ToolbarEvents Clicked="@Toggle"></ToolbarEvents>
            <ToolbarItems>
                <ToolbarItem PrefixIcon="e-tbar-menu-icon tb-icons" TooltipText="Menu">
                </ToolbarItem>
                <ToolbarItem>
                    <Template>
                        <div class="e-folder">
                            <div class="e-folder-name">Sections</div>
                        </div>
                    </Template>
                </ToolbarItem>
                <ToolbarItem Align="@ItemAlign.Right">
                    <Template>
                        <div>
                            <div class="e-user-name">@AppState.User.Email</div>
                        </div>
                    </Template>
                </ToolbarItem>
                <ToolbarItem CssClass="e-custom" Align="@ItemAlign.Right">
                    <Template>
                        <div class="image-container">
                        </div>
                    </Template>
                </ToolbarItem>
            </ToolbarItems>
        </SfToolbar>
    </div>
    <div class="maincontent">
        <div class="content">Hello world</div>
    </div>
</div>

@*Initialize the Sidebar component*@
<SfSidebar Width="260px" Target="@Target" Position="@Position" HtmlAttributes="@HtmlAttribute" @bind-IsOpen="SidebarToggle">
    <ChildContent>
        <SfTreeView TValue="SectionTreeItem">
            <TreeViewEvents TValue="SectionTreeItem" NodeSelecting="BeforeSelect" NodeSelected="OnSelect"></TreeViewEvents>
            <TreeViewFieldsSettings Id="Id" Text="Text" Selected="Selected" ParentID="Pid" HasChildren="HasChild" Expanded="Expanded" DataSource="@ListData"></TreeViewFieldsSettings>
        </SfTreeView>
    </ChildContent>
</SfSidebar>
@code
{
    [CascadingParameter] public AppState AppState { get; set; }

    public List<SectionTreeItem> ListData = new List<SectionTreeItem>();
    public string Target = ".maincontent";    
    public bool SidebarToggle = false;

    SidebarPosition Position { get; set; } = SidebarPosition.Left;
    XmlNode datasheetNodes;
    int IdCounter = 1;
    int parentId = 0;

    Dictionary<string, object> HtmlAttribute = new Dictionary<string, object>()
    {
        { "class", "default-sidebar" }
    };

    protected override async Task OnInitializedAsync()
    {
        await base.OnInitializedAsync();

        try
        {
            XmlDocument xDoc = AppState.DocumentSetXml;
            datasheetNodes = xDoc.DocumentElement.SelectSingleNode("Datasheet");

            foreach (XmlNode node in datasheetNodes.ChildNodes)
            {
                parentId++;

                BaseEntity entity = AppState.SchemaElements.Where(s => s.NodeName == node.Name).FirstOrDefault();

                if (entity is not null)
                {                    
                    ListData.Add(new SectionTreeItem() 
                    { 
                        Id = parentId.ToString(), 
                        Text = entity.DisplayName, 
                        Description = entity.Comments, 
                        HasChild = node.HasChildNodes,
                        Expanded = false});                      

                    if (node.HasChildNodes)
                    {  
                        foreach (XmlNode childnode in node)
                        {  
                            if (childnode.HasChildNodes)
                            {
                                BaseEntity childentity = AppState.SchemaElements.Where(s => s.NodeName == childnode.Name).FirstOrDefault();

                                if (childentity is not null)
                                {
                                    ListData.Add(new SectionTreeItem() { 
                                            Id = (IdCounter++).ToString(), 
                                            Pid = parentId.ToString(), 
                                            Text = childentity.DisplayName, 
                                            Description = childentity.Comments, 
                                            HasChild = false ,
                                            Expanded = false});                 
                                }
                            }
                        }
                    }
                }
            }

        }
        catch (Exception ex)
        {
            Console.Write(ex.Message);
        }

    }

    public void Toggle(ClickEventArgs args)
    {
        if (args.Item.TooltipText == "Menu")
        {
            SidebarToggle = !SidebarToggle;
        }
    }

    public void OnListSelect(Syncfusion.Blazor.Lists.ClickEventArgs<SectionTreeItem> args)
    {
        args.Cancel = true;
    }

    public void BeforeSelect(NodeSelectEventArgs args)
    {
        if (args.NodeData.Text == "Favorites" || args.NodeData.Text == "John")
        {
            args.Cancel = true;
        }
    }

    public void OnSelect(NodeSelectEventArgs args)
    {
        Console.WriteLine(args.NodeData);

        // StateHasChanged();
    }


    public class SectionTreeItem
    {
        public string Id { get; set; }
        public string Pid { get; set; }
        public string Text { get; set; }
        public string Description { get; set; }
        public bool HasChild { get; set; }
        public bool Selected { get; set; }
        public bool Expanded { get; set; }
    }

}
