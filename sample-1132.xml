    <FirstAidMeasures>
      <DescriptionOfFirstAidMeasures>
        <FirstAidInhalation PhraseId="EE300">
          <FullText>Verletzten unter Selbstschutz aus dem Gefahrenbereich bringen.</FullText>
        </FirstAidInhalation>
        <FirstAidInhalation PhraseId="EE408">
          <FullText>Bei Atemnot Sauerstoff inhalieren lassen.</FullText>
        </FirstAidInhalation>
        <FirstAidInhalation PhraseId="EE518">
          <FullText>Ärztliche Behandlung.</FullText>
        </FirstAidInhalation>
        <FirstAidSkin PhraseId="EH220">
          <FullText>Verunreinigte Kleidung, auch Unterwäsche und Schuhe, sofort ausziehen; auf Selbstschutz achten.</FullText>
        </FirstAidSkin>
        <FirstAidSkin PhraseId="EH260">
          <FullText>Haut mit viel Wasser spülen.</FullText>
        </FirstAidSkin>
        <FirstAidSkin PhraseId="EH501">
          <FullText>Ärztliche Behandlung.</FullText>
        </FirstAidSkin>
        <FirstAidEye PhraseId="EA180">
          <FullText>Augen unter Schutz des unverletzten Auges sofort ausgiebig (mind. 10 Minuten) bei geöffneten Augenlidern mit Wasser spülen.</FullText>
        </FirstAidEye>
        <FirstAidEye PhraseId="EA406">
          <FullText>Augenärztliche Behandlung.</FullText>
        </FirstAidEye>
        <FirstAidIngestion PhraseId="EV360">
          <FullText>Sofortiges kräftiges Ausspülen des Mundes.</FullText>
        </FirstAidIngestion>
        <FirstAidIngestion PhraseId="EV370">
          <FullText>Wasser in kleinen Schlucken trinken lassen (Verdünnungseffekt).</FullText>
        </FirstAidIngestion>
        <FirstAidIngestion PhraseId="EV502">
          <FullText>Ärztliche Behandlung.</FullText>
        </FirstAidIngestion>
      </DescriptionOfFirstAidMeasures>
      <MedicalAttentionAndSpecialTreatmentNeeded>
        <SpecificAntidotes PhraseId="EZ514">
          <FullText>Antidot: Toluidinblau.</FullText>
        </SpecificAntidotes>
      </MedicalAttentionAndSpecialTreatmentNeeded>
    </FirstAidMeasures>
